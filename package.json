  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "target": "es6",
        "jsx": "react"
      }
    },
    "mapCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "moduleDirectories": [
      "node_modules",
      "frontend/src",
      "backend"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ]
  }
