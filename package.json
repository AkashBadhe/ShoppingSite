  "scripts": {
    "build-dll": "webpack --color --config webpack.config.dll.babel.js",
    "build-prod": "webpack --color --config webpack.config.prod.babel.js",
    "profile": "profile=true webpack --color --config webpack.config.prod.babel.js",
    "server": "babel-node backend/server.js",
    "test": "jest --no-cache --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "deploy": "heroku container:push web",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn coveralls"
  },
